{
    "env": {
        "browser": true,
        "node": true,
        "es2021": true,
        "jquery": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 12
    },
    "plugins": [
        "html"
    ],
    "rules": {
        "no-unused-vars": "off",
        "no-undef": "error"
    },
    "globals": {
        "requireAuth": "readonly",
        "getUsers": "readonly",
        "showToast": "readonly",
        "updateUser": "readonly",
        "addUser": "readonly",
        "deleteUser": "readonly",
        "MENU_ITEMS": "readonly",
        "NOODLE_OPTIONS": "readonly",
        "MEAT_OPTIONS": "readonly",
        "VEGGIE_OPTIONS": "readonly",
        "EXTRA_OPTIONS": "readonly",
        "ALL_INGREDIENTS": "readonly",
        "guestId": "readonly",
        "currentMenuItem": "writable",
        "cart": "writable",
        "updateCartBadge": "readonly",
        "renderMenu": "readonly",
        "formatDate": "readonly",
        "onTableChange": "readonly",
        "showAlert": "readonly",
        "goToMenu": "readonly",
        "closeAlert": "readonly",
        "THAI_MONTHS": "readonly",
        "MENU_EMOJIS": "readonly",
        "getOrders": "readonly",
        "saveOrders": "readonly",
        "saveOrder": "readonly",
        "saveIngredientUsage": "readonly",
        "currentTable": "readonly",
        "closeConfirmOrder": "readonly",
        "showPage": "readonly",
        "getDateKey": "readonly",
        "dStr": "writable",
        "getMenuItem": "readonly",
        "getExtraItem": "readonly",
        "html2pdf": "readonly",
        "formatDateThai": "readonly",
        "renderCart": "readonly",
        "Swal": "readonly",
        "Chart": "readonly"
    }
}